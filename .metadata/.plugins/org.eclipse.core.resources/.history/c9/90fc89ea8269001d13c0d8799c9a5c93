

import java.util.List;


public class LibraryService {
	String libraryName;
	String address;
	List<Book> books;
	
	public LibraryService(String libraryName, String address, List<Book> books) {
		super();
		this.libraryName = libraryName;
		this.address = address;
		this.books = books;
	}
	
	public List<Book> getBooks() {
		return books;
		
	}
}
